  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0-rc.11",
    "moment": "^2.22.2",
    "v-click-outside": "^2.0.2",
    "vue-feather-icons": "^4.7.1",
    "vue-good-table": "^2.16.0",
